<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.waterball.life_simulator2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainGame" />
        <activity android:name=".Memo.Memo_Activity">
            <intent-filter>
                <action android:name="android.intent.action.Edit" />
                <action android:name="android.intent.action.Add" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckList.CheckList_Activity">
            <intent-filter>
                <action android:name="android.intent.action.EDIT"/>
            </intent-filter>
        </activity>
        <activity android:name=".Memo.Memo_item_page" />
        <activity android:name=".Memo.Memo_Item_Content"/>
        <activity android:name=".CheckList.CheckList_Add_Activity" />
        <activity android:name=".CheckList.CheckList_Add_Reset_Activity">
        </activity>
    </application>

</manifest>